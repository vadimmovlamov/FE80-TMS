idth: 134px;\r\n    text-align: center;\r\n    font-style: normal;\r\n    font-weight: 700;\r\n    font-size: 13px;\r\n    text-transform: uppercase;\r\n\r\n    &:hover{\r\n        border-bottom: 3px solid #BABBC5;\r\n    }\r\n    &:active{\r\n        color: #E71414;\r\n        border-bottom: 3px solid #E71414;\r\n    }\r\n}\r\n.header__link{\r\n    text-decoration: none;\r\n    color: $color-white;\r\n    &:active{\r\n        color: #E71414;\r\n    }\r\n}\r\n.header__active{\r\n    background: #2C3154;\r\n}\r\n.header__burger{\r\n    display: none;\r\n    width: 24px;\r\n    height: 18px;\r\n    position: relative;\r\n    margin: 19px 16px;\r\n}\r\n.burger__line{\r\n    display: block;\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 2px;\r\n    background-color: $color-white;\r\n    left: 0;\r\n}\r\n.burger__line-1{\r\n    top: 0;\r\n}\r\n.burger__line-2{\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n}\r\n.burger__line-3{\r\n    bottom: 0;\r\n}\r\n\r\n@media (max-width: 1024px) {\r\n    .header__item{\r\n        width: 112px;\r\n    }\r\n}\r\n@media (max-width: 780px){\r\n    .header__item{\r\n        width: 95px;\r\n    }\r\n}\r\n@media (max-width: 768px) {\r\n    .header__wrapper{\r\n        height: 56px;\r\n        border: none;\r\n    }\r\n    .header__logo {\r\n        a{\r\n            display: block;\r\n            width: 0;\r\n            height: auto;\r\n        }\r\n        img{\r\n            width: 130px;\r\n            height: 56px;\r\n        }\r\n    }\r\n    .header__burger{\r\n        display: block;\r\n    }\r\n    .header__nav{\r\n        width: 100%;\r\n        height: 100%;\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        padding-top: 84px;\r\n        background: url(../img/intro-bg-min.png) no-repeat center;\r\n        background-size: cover;\r\n        z-index: 10;\r\n        transform: translateX(100%);\r\n        transition: 0.2s all linear;\r\n    }\r\n    .header__nav_active{\r\n        transform: translateX(0);\r\n    }\r\n    .header__item{\r\n        height: 63px;\r\n        line-height: 63px;\r\n        width: 100%;\r\n        text-align: start;\r\n        padding: 0 20px;\r\n\r\n        &:hover{\r\n            border-bottom: none;\r\n            background: #d0d1d1;\r\n        }\r\n        &:active{\r\n            border-bottom: none;\r\n        }\r\n    }\r\n    .header__link{\r\n        font-size: 18px;\r\n    }\r\n    .header__nav-close{\r\n        width: 24px;\r\n        height: 24px;\r\n        position: absolute;\r\n        top: 19px;\r\n        right: 31px;\r\n        z-index: 11;\r\n    }\r\n    .header__nav-close-line{\r\n        display: block;\r\n        width: 100%;\r\n        height: 2px;\r\n        background-color: $color-white;\r\n        position: absolute;\r\n        top: 50%;\r\n    }\r\n    .header__nav-close-line:first-child{\r\n        transform: translateY(-50%) rotate(45deg);\r\n    }\r\n    .header__nav-close-line:last-child{\r\n        transform: translateY(-50%) rotate(-45deg);\r\n    }\r\n    .header__logo{\r\n        z-index: 15;\r\n    }\r\n    .header__link:hover{\r\n        color: #2C3154;\r\n    }\r\n    .header__link:active{\r\n        color: none;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7c14971f23564c61f70a01125f8a54ca","cacheData":{"env":{}}}